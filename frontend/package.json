    "@types/dompurify": "^3.0.5",
    "bad-words": "^4.0.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.24",
    "isomorphic-dompurify": "^2.35.0",
    "jspdf": "^2.5.2",
    "jszip": "^3.10.1",
    "next": "15.5.7",
    "posthog-js": "^1.315.0",
    "posthog-node": "^5.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-swipeable": "^7.0.2",
    "recharts": "^2.15.4",
    "stripe": "^19.1.0",
    "web-vitals": "^4.2.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
