{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tag": "node scripts/dev-filter.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:canary": "playwright test tests/smoke.spec.ts",
    "test:comprehensive": "playwright test -c playwright.comprehensive.config.ts",
    "test:comprehensive:full": "playwright test -c playwright.comprehensive.config.ts && node scripts/comprehensive-test-summary.mjs",
    "test:comprehensive:report": "playwright show-report test-results/comprehensive-html-report",
    "test:comprehensive:summary": "node scripts/comprehensive-test-summary.mjs",
    "test:unit": "tsx tests/unit/canonicalize.test.ts && tsx tests/unit/openai-params.test.ts && tsx tests/unit/layer0-gate.test.ts && tsx tests/unit/pricing.test.ts && tsx tests/unit/panic-switches.test.ts && tsx tests/unit/ai-usage-route.test.ts && tsx tests/unit/mulligan-trap-hands.test.ts",
    "verify:ai": "node scripts/verify-ai-cost-quality.mjs",
    "verify:ai:strict": "node scripts/verify-ai-cost-quality.mjs --require-db",
    "backfill:ai-cost": "node scripts/backfill-ai-usage-cost.mjs",
    "test:unit:openai": "tsx tests/unit/openai-params.test.ts",
    "test:assistant": "tsx tests/unit/build_assistant.test.ts",
    "seo:check": "node scripts/seo-check.mjs",
    "color-tests": "node scripts/run-color-regressions.mjs",
    "chat-tests": "node scripts/run-chat-regressions.mjs",
    "validate-analytics-excluded": "tsx scripts/validate-analytics-excluded-paths.ts"
  },
  "dependencies": {
